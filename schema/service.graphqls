input MutationServiceInput {
    cmd: Cmd!
    name: String!
    ip: String!
    port: Int!
}

input MutationNodeInput {
    ip: String!
    port: Int!
}

type NestService {
    config: String!
    statusContent: String!
    name: String!
    ip: String!
    port: Int!
    state: State!
}

type Service {
    name: String!
    nodes: [NestNode!]
}

type MutationService {
    cmd: Cmd!
    error: String!
    ip: String!
    name: String!
    port: Int!
    success: Boolean!
}